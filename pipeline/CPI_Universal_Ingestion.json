{
  "name": "CPI_Universal_Ingestion",
  "properties": {
    "activities": [
      {
        "name": "FRED_CPI_Full_Collection",
        "type": "SynapseNotebook",
        "dependsOn": [],
        "policy": {
          "timeout": "0.12:00:00",
          "retry": 0,
          "retryIntervalInSeconds": 30,
          "secureOutput": false,
          "secureInput": false
        },
        "userProperties": [],
        "typeProperties": {
          "notebook": {
            "referenceName": "FRED_CPI_Full_Collection",
            "type": "NotebookReference"
          },
          "parameters": {
            "release_id": {
              "value": "@pipeline().parameters.release_id",
              "type": "string"
            }
          },
          "sparkPool": {
            "referenceName": "spack2",
            "type": "BigDataPoolReference"
          },
          "executorSize": "Medium",
          "executorCores": 4,
          "conf": {
            "spark.dynamicAllocation.enabled": false,
            "spark.dynamicAllocation.minExecutors": 2,
            "spark.dynamicAllocation.maxExecutors": 4
          },
          "driverSize": "Medium",
          "driverCores": 4,
          "numExecutors": 2
        }
      }
    ],
    "parameters": {
      "release_id": {
        "type": "string",
        "defaultValue": "10"
      }
    },
    "annotations": []
  }
}