{
  "name": "CPI_Universal_Ingestion",
  "properties": {
    "activities": [
      {
        "name": "FRED_Azure_Function_Call",
        "type": "WebActivity",
        "dependsOn": [],
        "policy": {
          "timeout": "0.12:00:00",
          "retry": 0,
          "retryIntervalInSeconds": 30,
          "secureOutput": false,
          "secureInput": false
        },
        "userProperties": [],
        "typeProperties": {
          "url": "https://fred-function-app.azurewebsites.net/api/fred_ingestion",
          "method": "POST",
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "release_id": "@pipeline().parameters.release_id"
          },
          "authentication": {
            "type": "MSI",
            "resource": "https://fred-function-app.azurewebsites.net"
          }
        }
      }
    ],
    "parameters": {
      "release_id": {
        "type": "string",
        "defaultValue": "10"
      }
    },
    "annotations": []
  }
}